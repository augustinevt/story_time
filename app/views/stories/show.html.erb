<h1><%= @story.title %></h1>

<h3><%= link_to 'Add New Contribution', new_story_contribution_path(@story)%></h3>
<p>
  <% @story.contributions.each do |contribution| %>

  <%= contribution.content %>

  <%end%>
</p>

<p><%= link_to 'Edit', edit_story_path(@story) %></p>
<p><%= link_to 'Delete', story_path(@story),
                          data: {confirm: "You sure?",
                          method: "delete"}%></p>
